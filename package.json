{
  "name": "gramify-backend",
  "version": "1.0.0",
  "description": "Backend API for an Instagram clone application, featuring user authentication, photo uploads, likes, comments, and real-time notifications.",
  "main": "index.js",
  "type": "module",
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "seed": "node src/prisma/seed.js",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:reset": "prisma migrate reset"
  },
  "keywords": [
    "instagram",
    "social-media",
    "social-network"
  ],
  "author": "Syed Noman Ali",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "imagekit": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "socket.io": "^4.8.1",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "prisma": "^6.13.0"
  }
}
